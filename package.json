{
  "name": "saunottaja-discord-bot",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "axios": "0.21.1",
    "discord.js": "12.5.1",
    "dotenv": "8.2.0",
    "lodash": "4.17.21",
    "pg": "8.5.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.4.0",
    "unique-random": "^2.1.0"
  },
  "devDependencies": {
    "@types/jasmine": "3.6.3",
    "@types/lodash": "4.14.168",
    "@types/pg": "7.14.9",
    "copyfiles": "2.4.1",
    "jasmine": "3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "0.3.3",
    "rimraf": "3.0.2",
    "ts-node-dev": "1.1.6",
    "typescript": "4.1.3"
  },
  "scripts": {
    "start": "tsnd --respawn --files ./src/main.ts",
    "clean": "npx rimraf ./dist",
    "copy-resources": "npx copyfiles -u 1 \"./src/resources/**\" \"./dist\"",
    "compile": "npx tsc",
    "build": "npm run clean && npm run compile && npm run copy-resources",
    "postbuild": "cp package.json ./dist/package.json && cd dist && npm install --production",
    "test": "npx jasmine-ts --config=jasmine.json",
    "watch": "npx tsc --watch"
  },
